{
  ":configurator:resource-version": 1,

	"MQTTService~read":
	{
		"brokerUrl" : "$[env:MQTT_HOST_PROTOCOL]$[prop:MQTT_HOST_PROTOCOL;default=]://$[env:MQTT_READ_HOST]$[prop:MQTT_READ_HOST;default=]:$[env:MQTT_READ_PORT]$[prop:MQTT_READ_PORT;default=]",
		"username" : "$[env:MQTT_FULL_USER]$[prop:MQTT_FULL_USER;default=]",
		"password" : "$[env:MQTT_FULL_PWD]$[prop:MQTT_FULL_PWD;default=]",
		"id" : "read"
	},
	"MQTTService~write":
	{
		"brokerUrl" : "$[env:MQTT_HOST_PROTOCOL]$[prop:MQTT_HOST_PROTOCOL;default=]://$[env:MQTT_WRITE_HOST]$[prop:MQTT_WRITE_HOST;default=]:$[env:MQTT_WRITE_PORT]$[prop:MQTT_WRITE_PORT;default=]",
		"username" : "$[env:MQTT_FULL_USER]$[prop:MQTT_FULL_USER;default=]",
		"password" : "$[env:MQTT_FULL_PWD]$[prop:MQTT_FULL_PWD;default=]",
		"id" : "write"
	},
	"MQTTService~full":
	{
		"brokerUrl" : "$[env:MQTT_HOST_PROTOCOL]$[prop:MQTT_HOST_PROTOCOL;default=]://$[env:MQTT_FULL_HOST]$[prop:MQTT_FULL_HOST;default=]:$[env:MQTT_FULL_PORT]$[prop:MQTT_FULL_PORT;default=]",
		"username" : "$[env:MQTT_FULL_USER]$[prop:MQTT_FULL_USER;default=]",
		"password" : "$[env:MQTT_FULL_PWD]$[prop:MQTT_FULL_PWD;default=]",
		"id" : "full"
	}
}